  <transitionItemsCnt>%%Session.NumberOfTransitionItemsCheckedIn%%</transitionItemsCnt>
  <successful>
    $$Session.CheckedInItems{<itemRecord>
      <title>%%Checkin.ItemTitle%%</title>
      <barcode>%%Checkin.ItemId%%</barcode>
      <message>%%Checkin.ScreenMessage%%</message>
      <alertType>%%Checkin.ItemAlertType%%</alertType>
      <destinationLocation>%%Checkin.ItemDestinationLocation%%</destinationLocation>
    </itemRecord>}$$
  </successful>
</checkinList>